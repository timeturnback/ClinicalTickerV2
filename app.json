{
  "expo": {
    "name": "ClinicalTickerV2",
    "version": "1.0.5",
    "description": "Clinical Ticker by Dr Van Hai Long",
    "slug": "ClinicalTickerV2",
    "privacy": "unlisted",
    "sdkVersion": "30.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "ebitvn.medical.clinicaltickerv2"
    },
    "android": {
      "package": "ebitvn.medical.clinicaltickerv2"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "packagerOpts": {
      "assetExts": [
        "db"
      ]
    }
  }
}